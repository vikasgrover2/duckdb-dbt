SET SESSION AUTHORIZATION replication;

CREATE TABLE spark_ods_2.ps_cf_attrib_value (
    setid character varying(5) NOT NULL ENCODE lzo,
    fieldname character varying(18) NOT NULL ENCODE lzo,
    cf_attribute character varying(15) NOT NULL ENCODE lzo,
    cf_attrib_value character varying(20) NOT NULL ENCODE lzo,
    descr60 character varying(60) NOT NULL ENCODE lzo,
    load_timestamp timestamp without time zone default sysdate,
    PRIMARY KEY (setid, fieldname, cf_attribute, cf_attrib_value)
)
DISTSTYLE ALL;

